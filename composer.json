{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "anlutro/l4-settings": "^1.1.1",
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "benwilkins/laravel-fcm-notification": "^4.0",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^6.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "~6.0",
        "infyomlabs/adminlte-templates": "^2.0",
        "laravel/cashier": "^12.6",
        "laravel/framework": "^7.29",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.5",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^8.3",
        "orangehill/iseed": "^2.6",
        "paragonie/sodium_compat": "^1.19",
        "prettus/l5-repository": "^2.7.9",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "razorpay/razorpay": "^2.5",
        "smartervision/laravel-generator": "dev-master",
        "smartervision/laravel-installer": "dev-home_services",
        "smartervision/laravel-langman": "dev-master",
        "spatie/laravel-medialibrary": "^7.19",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-translatable": "^4.6",
        "spatie/opening-hours": "^2.10",
        "srmklive/paypal": "^1.8.0",
        "tymon/jwt-auth": "^1.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
